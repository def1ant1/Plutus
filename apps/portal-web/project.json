{
  "name": "portal-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portal-web/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": ["next"]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/portal-web/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/portal-web/.next/!(cache)"
      ],
      "options": {
        "cwd": "apps/portal-web",
        "tty": false,
        "command": "next build"
      },
      "configurations": {},
      "parallelism": true
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/portal-web",
        "command": "next dev"
      },
      "configurations": {},
      "continuous": true,
      "parallelism": true
    },
    "start": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "cwd": "apps/portal-web",
        "command": "next start"
      },
      "configurations": {},
      "continuous": true,
      "parallelism": true
    },
    "serve-static": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "cwd": "apps/portal-web",
        "command": "next start"
      },
      "configurations": {},
      "continuous": true,
      "parallelism": true
    },
    "build-deps": {
      "executor": "nx:noop",
      "dependsOn": ["^build"],
      "options": {},
      "configurations": {},
      "parallelism": true
    },
    "watch-deps": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-deps"],
      "options": {
        "command": "pnpm exec nx watch --projects portal-web --includeDependentProjects -- pnpm exec nx build-deps portal-web"
      },
      "configurations": {},
      "continuous": true,
      "parallelism": true
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p apps/portal-web/tsconfig.json --noEmit"
      }
    }
  }
}
