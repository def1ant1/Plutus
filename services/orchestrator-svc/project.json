{
  "name": "orchestrator-svc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/orchestrator-svc/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "outputs": ["{workspaceRoot}/dist/services/orchestrator-svc"],
      "options": {
        "command": "pnpm exec tsc -p services/orchestrator-svc/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "continuous": true,
      "options": {
        "command": "node dist/services/orchestrator-svc/main.js"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec tsc -p services/orchestrator-svc/tsconfig.app.json --noEmit"
      }
    }
  }
}
